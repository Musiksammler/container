{
    "name": "northwoods/container",
    "description": "Container wrapper for Auryn dependency injector",
    "license": "MIT",
    "keywords": [
        "auyrn",
        "container",
        "psr-11"
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/shadowhand/zend-container-config-test"
        }
    ],
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require": {
        "php": ">=7.1",
        "psr/container": "^1.0",
        "rdlowrey/auryn": "^1.1"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^4.0",
        "phpunit/phpunit": "^7.0.3",
        "squizlabs/php_codesniffer": "^3.2",
        "zendframework/zend-container-config-test": "dev-fix/factory-test-return"
    },
    "autoload": {
        "psr-4": {
            "Northwoods\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwoods\\Container\\": "tests/"
        }
    },
    "scripts": {
        "check": "phpcs --standard=PSR2 src/ tests/",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
