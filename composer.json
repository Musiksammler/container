{
    "name": "northwoods/container",
    "description": "Container wrapper for Auryn dependency injector",
    "type": "library",
    "license": "MIT",
    "provide": {
        "psr/container-implementation": "^1.0"
    },
    "require": {
        "php": ">=7.1",
        "rdlowrey/auryn": "^1.1",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "eloquent/phony-phpunit": "^3.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Northwoods\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Northwoods\\Container\\": "tests/"
        }
    },
    "scripts": {
        "check": "phpcs --standard=PSR2 src/ tests/",
        "test": "phpunit"
    }
}
